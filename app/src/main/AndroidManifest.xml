<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.happyroutine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".social"></activity>
        <activity android:name=".settings" />
        <activity android:name=".user_information" />
        <activity android:name=".forgot_my_password" />
        <activity android:name=".Estadisticas_main" />
        <activity android:name=".Estadisticas_trainning_days"/>
        <activity android:name=".Estadisticas_weight"/>
        <activity android:name=".Navigation_bar_main"/>
        <activity android:name=".sign_up" />
        <activity android:name=".log_in" />
        <activity android:name=".routine" />
        <activity android:name=".exercise" />
        <activity android:name=".diet" />
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>